{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for the Codidact Startup Coach",
  "main": "src/index.ts",
  "scripts": {
    "start": "echo \"Starting index.ts\" && ts-node src/index.ts",
    "start:dev": "echo \"Starting index.ts with nodemon\" && nodemon src/index.ts",
    "test": "echo \"Starting Jest tests\" && jest --detectOpenHandles",
    "test:watch": "echo \"Starting Jest tests in watch mode\" && jest --detectOpenHandles --watchAll",
    "test:coverage": "echo \"Generating code coverage report\" && jest --coverage --detectOpenHandles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix src/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.2",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-newlines": "^1.3.4",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.1",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
