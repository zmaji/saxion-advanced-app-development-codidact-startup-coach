    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@popperjs/core": "^2.11.8",
    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^8.0.1",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "jest": "^29.7.0",
    "jwt-decode": "^4.0.0",
